CREATE TABLE category (id BIGINT AUTO_INCREMENT, name VARCHAR(100), display_order TINYINT, extra_info VARCHAR(255), treatment_group_id BIGINT, created_at DATETIME NOT NULL, updated_at DATETIME NOT NULL, PRIMARY KEY(id)) ENGINE = INNODB;
CREATE TABLE facebook_album (id BIGINT AUTO_INCREMENT, fb_id TEXT, name TEXT, link TEXT, cover_photo TEXT, photo_count TEXT, type TEXT, fb_created_time TEXT, fb_updated_time TEXT, num_comments TEXT, live_in_photos TINYINT(1), created_at DATETIME NOT NULL, updated_at DATETIME NOT NULL, PRIMARY KEY(id)) ENGINE = INNODB;
CREATE TABLE facebook_photo (id BIGINT AUTO_INCREMENT, app_album_id BIGINT, fb_id TEXT, fb_album_id TEXT, name TEXT, picture TEXT, source TEXT, height TEXT, width TEXT, image_one_height TEXT, image_one_width TEXT, image_one_source TEXT, image_two_height TEXT, image_two_width TEXT, image_two_source TEXT, image_three_height TEXT, image_three_width TEXT, image_three_source TEXT, image_four_height TEXT, image_four_width TEXT, image_four_source TEXT, link TEXT, icon TEXT, fb_created_time TEXT, fb_updated_time TEXT, position BIGINT, comments text, live_in_photos TINYINT(1), live_on_homepage TINYINT(1), created_at DATETIME NOT NULL, updated_at DATETIME NOT NULL, PRIMARY KEY(id)) ENGINE = INNODB;
CREATE TABLE mailing_list (id BIGINT AUTO_INCREMENT, email VARCHAR(255), created_at DATETIME NOT NULL, updated_at DATETIME NOT NULL, PRIMARY KEY(id)) ENGINE = INNODB;
CREATE TABLE photo (id BIGINT AUTO_INCREMENT, file_name TEXT, title TEXT, live TINYINT(1), created_at DATETIME NOT NULL, updated_at DATETIME NOT NULL, PRIMARY KEY(id)) ENGINE = INNODB;
CREATE TABLE product (id BIGINT AUTO_INCREMENT, name VARCHAR(100), price DECIMAL(20, 2), is_available TINYINT(1), category_id BIGINT, extra_info VARCHAR(255), created_at DATETIME NOT NULL, updated_at DATETIME NOT NULL, PRIMARY KEY(id)) ENGINE = INNODB;
CREATE TABLE special_offer (id BIGINT AUTO_INCREMENT, description_one VARCHAR(255), description_two VARCHAR(255), description_three VARCHAR(255), display_order TINYINT, front_page TINYINT(1), live TINYINT(1), PRIMARY KEY(id)) ENGINE = INNODB;
CREATE TABLE testimonial (id BIGINT AUTO_INCREMENT, statement VARCHAR(255), name VARCHAR(255), display_order INT, live TINYINT(1), created_at DATETIME NOT NULL, updated_at DATETIME NOT NULL, PRIMARY KEY(id)) ENGINE = INNODB;
CREATE TABLE treatment_group (id BIGINT AUTO_INCREMENT, name VARCHAR(255), display_order BIGINT, PRIMARY KEY(id)) ENGINE = INNODB;
